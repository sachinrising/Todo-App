<!-- #<p id="notice"><#%= notice %></p> -->

<h2 class="todo_list_title"><%= @todo_list.title %></h2>
<p class="todo_list_sub_title"><%= @todo_list.description %></p>

<div id="todo_items_wrapper">
	<!-- <#%= render @todo_list.todo_items %> -->
	<% @todo_items.each do |todo_item| %>
	<%= render todo_item %>
	<% end %>
	<div id="form">
		<%= render "todo_items/form" %>
	</div>
</div>

<!-- <#%= paginate @todo_items, param_name: :items_page %> -->

<div class="links">
	<%= link_to 'Edit', edit_todo_list_path(@todo_list), class: "newTodoList" %> |
	<%= link_to 'Delete', todo_list_path(@todo_list), class: "newTodoList", method: :delete, data: { confirm: "Are you sure?" }
 %> |
  <%= link_to 'Back', todo_lists_path %>
</div>
